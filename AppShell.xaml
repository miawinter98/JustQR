<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="JustQR.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:JustQR"
    FlyoutBehavior="Disabled" Title="JustQR"
    xmlns:strings="clr-namespace:JustQR.Resources">

    <TabBar>
        <ShellContent
            Title="{x:Static strings:AppResources.AppShell_TabTitle_Home}"
            ContentTemplate="{DataTemplate local:MainPage}" />

        <ShellContent
            Title="{x:Static strings:AppResources.AppShell_TabTitle_About}"
            ContentTemplate="{DataTemplate local:AboutPage}" />
    </TabBar>
</Shell>
